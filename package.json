{
  "name": "node-token-jwt",
  "main": "server.js",
  "scripts": {
    "dev-api-win": "nodemon server.js --exec babel-node"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-plugin-transform-builtin-extend": "^1.1.2",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-stage-3": "^6.24.1",
    "babel-register": "^6.26.0",
    "nodemon": "^1.17.3"
  },
  "dependencies": {
    "body-parser": "^1.18.2",
    "express": "^4.16.3",
    "express-validator": "^5.1.2",
    "import-export": "^1.0.1",
    "jsonwebtoken": "^8.2.1",
    "mocha": "^5.1.1",
    "mongodb": "^2.2.33",
    "morgan": "^1.9.0",
    "sha1": "^1.1.1"
  },
  "babel": {
    "presets": [
      "env",
      "stage-3"
    ],
    "plugins": [
      "transform-runtime",
      [
        "transform-builtin-extend",
        {
          "globals": [
            "Error",
            "Array"
